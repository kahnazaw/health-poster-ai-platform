{
  "name": "health-poster-ai-platform",
  "version": "1.0.0",
  "description": "منصة مؤسسية رسمية لتوليد بوسترات التوعية الصحية بالذكاء الاصطناعي",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate --schema=prisma/schema.prisma && npx prisma migrate deploy --schema=prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "SKIP_ENV_VALIDATION=1 npx prisma generate",
    "db:push": "npx prisma db push --accept-data-loss",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:status": "prisma migrate status",
    "postinstall": "SKIP_ENV_VALIDATION=1 npx prisma generate",
    "db:studio": "prisma studio",
    "db:init": "npx prisma db push --accept-data-loss && npx prisma db seed",
    "sync-db": "node scripts/sync-db.mjs",
    "force-admin": "node scripts/create-admin.mjs",
    "check-users": "node scripts/check-users.mjs",
    "seed": "prisma db seed",
    "check:production": "node scripts/check-production-ready.mjs"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-toast": "^1.1.5",
    "@types/qrcode": "^1.5.6",
    "bcryptjs": "^2.4.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.26",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.562.0",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-to-print": "^2.14.15",
    "recharts": "^3.6.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "prisma": "^7.2.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
